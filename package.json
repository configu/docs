    "react": "^18.2.0",
    "react-cookie-consent": "^8.0.1",
    "react-dom": "^18.2.0",
    "react-instantsearch-dom": "^6.38.1",
    "react-syntax-highlighter": "^15.5.0",
    "slugify": "^1.6.5",
    "tailwindcss": "^3.2.4"
